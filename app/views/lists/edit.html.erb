<h1>Edit List</h1>

<%= form_with model: @list, local: true do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="field">
    <%= form.label :image_url %>
    <%= form.text_field :image_url %>
  </div>
  <div class="actions">
    <%= form.submit 'Update List' %>
  </div>
<% end %>

<%= link_to 'Back to Lists', lists_path %>
